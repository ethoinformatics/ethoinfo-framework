{
  "name": "ethoinfo-framework",
  "version": "0.0.0",
  "description": "This sample uses browserify to organize the code in to modules, vash for client side rendering of templates and jQuery for everything else",
  "main": "src/registrar.js",
  "browserify": {
    "transform": [
      "vashify",
      "node-lessify"
    ]
  },
  "scripts": {
    "start": "./node_modules/.bin/nodemon -e js,vash,less ./bundler.js",
    "deploy": "cp -r public/* ./dist/"
  },
  "author": "ethoformatics",
  "license": "none",
  "bugs": {
    "url": "https://github.com/ethoinformatics/sample-ui-browserify/issues"
  },
  "homepage": "https://github.com/ethoinformatics/sample-ui-browserify",
  "dependencies": {
    "browserify": "^6.3.4",
    "d3-timeline": "git+ssh://git@github.com/ethoinformatics/d3-timeline#b53d26133bccbcb98625d1f091d603db779d3452",
    "date-math": "0.0.1",
    "ezuuid": "0.0.1",
    "iscroll": "^5.1.3",
    "jocal": "0.0.1",
    "jquery": "^2.1.1",
    "leaflet": "^0.7.3",
    "lodash": "^2.4.1",
    "merle": "^0.1.5",
    "moment": "^2.8.2",
    "node-lessify": "0.0.5",
    "pouchdb": "^3.0.6",
    "q": "^1.0.1",
    "rgba-generate": "^1.0.0",
    "vashify": "^0.3.3",
    "velocity-animate": "^1.2.1"
  },
  "devDependencies": {
    "nodemon": "^1.2.1"
  },
  "browser": {
    "app": "./src/app.js",
    "dom-ready": "./src/dom-ready.js",
    "geolocation": "./src/geolocation.js",
    "modal": "./src/views/modal/index.js",
    "create-new-dialog": "./src/views/create-new-dialog/index.js",
    "view-existing-dialog": "./src/views/view-existing-dialog/index.js",
    "breadcrumb": "./src/views/breadcrumb/index.js",
    "map": "./src/views/map/index.js",
    "edit-existing-dialog": "./src/views/edit-existing-dialog/index.js",
    "edit-existing-form": "./src/views/edit-existing-form/index.js",
    "activity-filter": "./src/views/activity-filter/index.js",
    "form-builder": "./src/form-builder/index.js",
    "timeline": "./src/timeline.js",
    "long-click": "./src/long-click.js",
    "sample-data": "./src/sample-data.json"
  }
}
