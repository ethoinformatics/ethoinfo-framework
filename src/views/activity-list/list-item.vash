 <div data-id="@model.id" class="item item-icon-right">
	@model.type
	@if (model.ending_time){
		<span data-ending-time="@model.ending_time" class="item-note">
			Ended @html.relativeTime(model.ending_time)
		</span>
	} else {
		<span data-starting-time="@model.starting_time" class="item-note">
			Started @html.relativeTime(model.starting_time)
		</span>
	}
	<i class="js-delete icon assertive ion-close-circled"></i>
</div>
